# Network Scanner configuration (.env format)
# Copy to .env and adjust values

# Path to SQLite DB file
sqlite_path=./scanner.sqlite

# Directory to store scan artifacts and outputs
data_dir=./data

# Masscan rate (packets per second)
rate=2048

# Path to nmap binary
nmap_path=nmap

# Default ports when tenant-specific ports are not set
# Comma-separated list and/or ranges, e.g. 22,80,443,1-1024
tcp_ports_default=22,80,443,1-1024
# For UDP defaults use comma-separated list, e.g. 53,69,123,161
udp_ports_default=53,123,161
